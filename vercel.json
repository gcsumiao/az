{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "ui/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py",
            "config": {
                "maxLambdaSize": "15mb",
                "memory": 3008,
                "maxDuration": 60
            }
        },
        {
            "src": "/(.*)",
            "dest": "/ui/$1"
        }
    ]
}
